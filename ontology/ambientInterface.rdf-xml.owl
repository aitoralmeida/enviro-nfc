@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl:    <http://www.w3.org/2003/11/swrl#> .
@prefix default:  <http://paginaspersonales.deusto.es/aitor.almeida/ambientInterface.owl#> .
@prefix protege:  <http://protege.stanford.edu/plugins/owl/protege#> .
@prefix xsp:     <http://www.owl-ontologies.com/2005/08/07/xsp.owl#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix swrlb:   <http://www.w3.org/2003/11/swrlb#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

default:Slide
      rdf:type owl:Class ;
      rdfs:subClassOf default:Widget ;
      owl:disjointWith default:Controler , default:Label , default:Button .

default:title
      rdf:type owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain default:Interface ;
      rdfs:range xsd:string .

default:endpoint
      rdf:type owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain default:Interface ;
      rdfs:range xsd:string .

default:Interface
      rdf:type owl:Class ;
      owl:disjointWith default:Widget .

default:Label
      rdf:type owl:Class ;
      rdfs:subClassOf default:Widget ;
      owl:disjointWith default:Slide , default:Controler , default:Button .

default:orientation
      rdf:type owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain default:Slide ;
      rdfs:range xsd:string .

default:limitStart
      rdf:type owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain default:Slide ;
      rdfs:range xsd:float .

default:position
      rdf:type owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain default:Widget ;
      rdfs:range xsd:string .

default:limitEnd
      rdf:type owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain default:Slide ;
      rdfs:range xsd:float .

<http://paginaspersonales.deusto.es/aitor.almeida/ambientInterface.owl>
      rdf:type owl:Ontology .

default:Widget
      rdf:type owl:Class ;
      owl:disjointWith default:Interface .

default:text
      rdf:type owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain
              [ rdf:type owl:Class ;
                owl:unionOf (default:Label default:Button)
              ] ;
      rdfs:range xsd:string .

default:composed_by
      rdf:type owl:ObjectProperty ;
      rdfs:domain default:Interface ;
      rdfs:range default:Widget .

default:action
      rdf:type owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain
              [ rdf:type owl:Class ;
                owl:unionOf (default:Label default:Button default:Slide)
              ] ;
      rdfs:range xsd:string .

default:Controler
      rdf:type owl:Class ;
      rdfs:subClassOf default:Widget ;
      owl:disjointWith default:Slide , default:Label , default:Button .

default:Button
      rdf:type owl:Class ;
      rdfs:subClassOf default:Widget ;
      owl:disjointWith default:Slide , default:Controler , default:Label .
